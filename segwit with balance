var bitcoinjs = require('bitcoinjs-lib');

function bech32Pair(){
	var NETWORK = bitcoinjs.networks.bitcoin;
	var wif = bitcoinjs.ECPair.makeRandom({network: NETWORK}).toWIF();
	var keyPair = bitcoinjs.ECPair.fromWIF(wif, NETWORK);
	var pubKey = keyPair.getPublicKeyBuffer();
	var scriptPubKey = bitcoinjs.script.witnessPubKeyHash.output.encode(bitcoinjs.crypto.hash160(pubKey))
	var address = bitcoinjs.address.fromOutputScript(scriptPubKey);  
	var adar = [];
	adar.push(address,wif);
	return JSON.stringify(adar);
}

